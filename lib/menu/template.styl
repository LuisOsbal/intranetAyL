.Menu
	display block
	top 0
	position fixed
	transition 0.5s
	box-shadow 0 4px 9px 0 rgba(50, 50, 50, 0.61)
	right 0
	left 0
	z-index 30
	width 100%
	height 3.5em
	&-rectangle
		position relative
		z-index 2
		margin-left 4em
		border-bottom 3.5em solid blueOsmatik
		border-left 3em solid transparent
	&-logoContainer
		display block
		position relative
		top 0
		margin .3em 0 0 0
	&-logoAYL
		width 100px
		position absolute
		left 1em
	&-logoHome
		width 150px
		position absolute
		right 1em
// @media screen and (min-width 600px)
// 	.Menu
// 		width 55%
// 		z-index 5
@media screen and (min-width 768px)
	.Menu
		width 60%
		height 5em
		z-index 5
		&-logoAYL
			width 140px
			position absolute
			left 1em
		&-logoHome
			width 250px
			position absolute
			right 1em
@media screen and (min-width 900px)
	.Menu
		width 70%
		z-index 5